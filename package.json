{
  "name": "angular-seed",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm install",
    "poststart": "npm run stop-http",
    "start-http": "http-server ./build -a 127.0.0.1 -c-1 -p 8001 -o; return 0",
    "stop-http": "kill-port --port 8001",
    "prebuild": "npm run start",
    "build": "webpack --config webpack.build.config.js -p & gulp build",
    "predev": "npm run start",
    "dev": "webpack --config webpack.dev.config.js --watch & gulp dev",
    "pretest": "npm install & npm install",
    "test": "karma start karma.conf.js",
    "preupdate-webdriver": "kill-port --port 8002 & npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "http-server ./build -a 127.0.0.1 -p 8002 -c-1 & protractor e2e-tests/protractor.conf.js",
    "lint": "eslint ./src; return 0",
    "jsdoc": "gulp docs"
  },
  "author": "Szymon Ryczek",
  "license": "MIT",
  "devDependencies": {
    "angular-jsdoc": "^1.5.0",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "eslint": "^3.18.0",
    "eslint-config-google": "^0.7.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-sass": "^3.1.0",
    "gulp-shell": "^0.6.3",
    "gulp-watch": "^4.3.11",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.5.2",
    "jsdoc": "^3.4.3",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-webpack": "^2.0.3",
    "kill-port": "^1.0.0",
    "local-web-server": "^1.2.7",
    "protractor": "^5.1.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "angular": "^1.6.3",
    "angular-animate": "^1.6.4",
    "angular-loader": "^1.6.3",
    "angular-mocks": "^1.6.3",
    "angular-resource": "^1.6.4",
    "angular-route": "^1.6.3"
  }
}
